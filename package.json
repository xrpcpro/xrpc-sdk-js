{
  "name": "@xrpc-pro/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for xRPC - Multi-chain RPC Gateway. Supports Ethereum, Polygon, Arbitrum, Optimism, Base, and more with full TypeScript support.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rpc",
    "ethereum",
    "blockchain",
    "web3",
    "json-rpc",
    "multi-chain",
    "polygon",
    "arbitrum",
    "optimism",
    "base",
    "beacon",
    "wss",
    "websocket",
    "typescript",
    "sdk",
    "api",
    "node",
    "browser"
  ],
  "author": {
    "name": "xRPC",
    "url": "https://app.xrpc.pro"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xrpcpro/xrpc-sdk-js.git"
  },
  "homepage": "https://app.xrpc.pro/docs",
  "bugs": {
    "url": "https://github.com/xrpcpro/xrpc-sdk-js/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rollup": "^4.9.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {}
}

